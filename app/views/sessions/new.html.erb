<h1>Login!</h1>

<p><% if flash[:error]%>
    <h3 style="color:red;"><%= flash[:error] %> </h3>
    <%end%>
</p>
<%= form_for(:user) do |f|%>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :password %>
  <%= f.password_field :password %>


  <%= f.submit "Log In" %>
<% end %>