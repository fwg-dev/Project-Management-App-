<%= render partial: 'layouts/errors' , locals: {object: @project} %>

<%= form_for(@project) do |f| %>

<div>
<%= f.label :Name %>
<%= f.text_field :name %>
</div>
<br>

<div>
<%= f.label :Completed? %>
<%= f.text_field :completion_status %>
</div>

<p> 
<%= f.submit %>
</p>
<% end %>